							</div>
							<!--end::Info-->

							{% block content_subheader_right %}{% endblock %}
						</div>
					</div>
					<!--end::Subheader-->
				{% endblock %}

				<!--begin::Content-->
				<div class="content d-flex flex-column flex-column-fluid" id="kt_content">
					{% block content_body %}{% endblock %}
				</div>
				<!--end::Content-->

